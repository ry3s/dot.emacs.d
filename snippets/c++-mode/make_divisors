# -*- mode: snippet -*-
# name: make_divisors
# key: make_divisors
# --
vector<int64_t> make_divisors(int64_t n) {
    vector<int64_t> res;

    for (int64_t i = 1LL; i * i <= n; ++i) {
        if (n % i == 0) {
            res.push_back(i);
            if (i * i != n) res.push_back(n / i);
        }
    }

    sort(res.begin(), res.end());
    return res;
}